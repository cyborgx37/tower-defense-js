{
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"alwaysStrict": true,
		"downlevelIteration": true,
		"esModuleInterop": true,
		"module": "AMD",
		"moduleResolution": "node",
		"noImplicitAny": true,
		"outDir": "build/ts",
		"rootDir": "src/js",
		"sourceMap": true,
		"strict": true,
		"target": "ES6",
	}
}